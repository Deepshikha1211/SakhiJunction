{
  "name": "sakhi-junction-workspace",
  "version": "1.0.0",
  "description": "Sakhi Junction - Women's Wellness Platform Workspace",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd mindful-message-haven-main/backend && npm run dev",
    "dev:frontend": "cd mindful-message-haven-main && npm run dev",
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd mindful-message-haven-main/backend && npm install",
    "install:frontend": "cd mindful-message-haven-main && npm install",
    "build": "npm run build:frontend",
    "build:frontend": "cd mindful-message-haven-main && npm run build",
    "start": "npm run start:backend",
    "start:backend": "cd mindful-message-haven-main/backend && npm start",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd mindful-message-haven-main/backend && npm test",
    "test:frontend": "cd mindful-message-haven-main && npm test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "women-health",
    "wellness",
    "healthcare",
    "community",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Jiya Damara",
  "license": "MIT"
}
